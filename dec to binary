#include <iostream>
#include<vector>
using namespace std;
int main() {
     int no ;
     cin>>no;
     vector<int>ans;
     while (no != 1) {
        int rem = no % 2;
        ans.push_back(rem);
        no = no / 2;
    }
    ans.push_back(no);
    
    for(int i=ans.size()-1;i>=0;i--){
        cout<<ans[i];
    }
    return 0;
}
