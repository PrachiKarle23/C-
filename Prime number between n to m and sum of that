#include <iostream>
#include <cmath>
using namespace std;

bool prime(int n)
{
   for(int i=2;i*i<=n;i++)
   {
       if(n%i == 0)
       {
          return false;
       }
   }
   return true;
}
int main() {
    int n,m;
    cin>>n>>m;
    int sum=0;
    for(int i=n;i<=m;i++)
    {
        if(prime(i)){
            // cout<<i<<" ";
            sum+=i;
        }
    }
    cout<<sum;
    return 0;
}
